{
  "expo": {
    "name": "padhai-app",
    "slug": "padhai-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "scheme": "padhai",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "deepLinking": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "enableHermes": true,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo.png",
        "backgroundColor": "#000000"
      },
      "package": "com.ayushkumar077.padhaiapp",
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "padhai",
              "host": "auth"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-secure-store",
      "expo-video"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "25ae5ffc-fdad-42df-b91a-2a5d46738dd6"
      }
    },
    "owner": "ayushkumar077",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/25ae5ffc-fdad-42df-b91a-2a5d46738dd6"
    }
  }
}
